{
  "name": "linkist",
  "main": "./lib/linkist",
  "version": "1.1.0",
  "description": "Insert and follow links between documents in your project",
  "keywords": [
    "link",
    "navigation",
    "hyperlink",
    "zettelkasten"
  ],
  "activationCommands": {
    "atom-workspace": "linkist:link"
  },
  "repository": "https://github.com/gladed/linkist",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "configSchema": {
    "filesToSearch": {
      "type": "string",
      "default": "*.md, *.txt",
      "description": "Comma-separated filename globs to search."
    }
  }
}
